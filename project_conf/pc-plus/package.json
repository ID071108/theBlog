    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.11.0",
    "happy-dom": "^9.20.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "release-it": "^15.10.3",
    "rollup-plugin-visualizer": "^5.9.0",
