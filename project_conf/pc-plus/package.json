    "vite-plugin-html": "^3.2.0",
    "vite-ssg": "^0.22.2",
    "vitest": "^0.31.1",
    "vue-tsc": "^1.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,.tsx,vue,html,md}": "eslint --config .eslintrc.js",
    "src/**/*.{css,less,scss}": "stylelint --config .stylelintrc.js",
    "*.{ts,tsx,js,json,html,yml,css,less,md}": "prettier --write"
  },
