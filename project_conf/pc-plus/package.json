    "animate.css": "^4.1.1",
    "axios": "^1.4.0",
    "default-passive-events": "^2.0.0",
    "echarts": "^5.4.2",
    "element-plus": "^2.3.4",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.35",
